        bot.send_message(message.chat.id, neprav, reply_markup=knop3)


def kno4(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–†–∞–∫–µ—Ç–∞" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("4q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–†–∞–∫–µ—Ç–∞-–Ω–æ—Å–∏—Ç–µ–ª—å ¬´–í–æ—Å—Ç–æ–∫¬ª –∑–∞–Ω–∏–º–∞–µ—Ç –æ—Å–æ–±–æ–µ –º–µ—Å—Ç–æ –≤ –∏—Å—Ç–æ—Ä–∏–∏ –æ—Ç–µ—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–π"
                                               " –∫–æ—Å–º–∏—á–µ—Å–∫–æ–π –ø—Ä–æ–≥—Ä–∞–º–º—ã, –≤–µ–¥—å –∏–º–µ–Ω–Ω–æ —ç—Ç–æ—Ç –∞–ø–ø–∞—Ä–∞—Ç 12 –∞–ø—Ä–µ–ª—è 1961 –≥–æ–¥–∞ "
                                               "–≤—ã–≤–µ–ª –Ω–∞ –æ–∫–æ–ª–æ–∑–µ–º–Ω—É—é –æ—Ä–±–∏—Ç—É ¬´–í–æ—Å—Ç–æ–∫-1¬ª, –Ω–∞ –∫–æ—Ç–æ—Ä–æ–º –Æ—Ä–∏–π –ì–∞–≥–∞—Ä–∏–Ω "
                                               "—Å–æ–≤–µ—Ä—à–∏–ª"
                                               " –ø–µ—Ä–≤—ã–π –≤ –∏—Å—Ç–æ—Ä–∏–∏ –ó–µ–º–ª–∏ –ø–∏–ª–æ—Ç–∏—Ä—É–µ–º—ã–π –ø–æ–ª–µ—Ç –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ. "
                                               "–ú–∞–∫–µ—Ç —Ä–∞–∫–µ—Ç—ã-–Ω–æ—Å–∏—Ç–µ–ª—è ¬´–í–æ—Å—Ç–æ–∫¬ª –±—ã–ª —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –Ω–∞ –í–î–ù–• 8 –∏—é–ª—è 1967 –≥–æ–¥–∞ "
                                               "‚Äî –≤ —á–µ—Å—Ç—å 50-–ª–µ—Ç–∏—è —Å–æ –¥–Ω—è –í–µ–ª–∏–∫–æ–π –û–∫—Ç—è–±—Ä—å—Å–∫–æ–π —Å–æ—Ü–∏–∞–ª–∏—Å—Ç–∏—á–µ—Å–∫–æ–π "
                                               "—Ä–µ–≤–æ–ª—é—Ü–∏–∏. –°–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –Ω–µ—Ç-–Ω–µ—Ç –Ω–µ –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ,"
                                               " –∞ –¥–µ–ª–∞—Ç—å –∫—Ä–∞—Å–∏–≤–µ–π—à–∏–µ —Ñ–æ—Ç–æ–∫–∞—Ä—Ç–æ—á–∫–∏ —Ä—è–¥–æ–º —Å –í–æ—Å—Ç–æ–∫–æ–º. –ê —á—Ç–æ–±—ã –≤–∞—à–∏"
                                               " –±–∞–ª–ª—ã –±—ã–ª–∏ –∑–∞—Å—á–∏—Ç–∞–Ω—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ —Ñ–æ—Ç–æ –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –≥—Ä—É–ø–ø—ã "
                                               "–í–ö.", reply_markup=men)
    elif "—Ä–∞–∫–µ—Ç–∞" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("4q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–†–∞–∫–µ—Ç–∞-–Ω–æ—Å–∏—Ç–µ–ª—å ¬´–í–æ—Å—Ç–æ–∫¬ª –∑–∞–Ω–∏–º–∞–µ—Ç –æ—Å–æ–±–æ–µ –º–µ—Å—Ç–æ –≤ –∏—Å—Ç–æ—Ä–∏–∏ –æ—Ç–µ—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–π"
                                               " –∫–æ—Å–º–∏—á–µ—Å–∫–æ–π –ø—Ä–æ–≥—Ä–∞–º–º—ã, –≤–µ–¥—å –∏–º–µ–Ω–Ω–æ —ç—Ç–æ—Ç –∞–ø–ø–∞—Ä–∞—Ç 12 –∞–ø—Ä–µ–ª—è 1961 –≥–æ–¥–∞ "
                                               "–≤—ã–≤–µ–ª –Ω–∞ –æ–∫–æ–ª–æ–∑–µ–º–Ω—É—é –æ—Ä–±–∏—Ç—É ¬´–í–æ—Å—Ç–æ–∫-1¬ª, –Ω–∞ –∫–æ—Ç–æ—Ä–æ–º –Æ—Ä–∏–π –ì–∞–≥–∞—Ä–∏–Ω "
                                               "—Å–æ–≤–µ—Ä—à–∏–ª"
                                               " –ø–µ—Ä–≤—ã–π –≤ –∏—Å—Ç–æ—Ä–∏–∏ –ó–µ–º–ª–∏ –ø–∏–ª–æ—Ç–∏—Ä—É–µ–º—ã–π –ø–æ–ª–µ—Ç –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ. "
                                               "–ú–∞–∫–µ—Ç —Ä–∞–∫–µ—Ç—ã-–Ω–æ—Å–∏—Ç–µ–ª—è ¬´–í–æ—Å—Ç–æ–∫¬ª –±—ã–ª —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –Ω–∞ –í–î–ù–• 8 –∏—é–ª—è 1967 –≥–æ–¥–∞ "
                                               "‚Äî –≤ —á–µ—Å—Ç—å 50-–ª–µ—Ç–∏—è —Å–æ –¥–Ω—è –í–µ–ª–∏–∫–æ–π –û–∫—Ç—è–±—Ä—å—Å–∫–æ–π —Å–æ—Ü–∏–∞–ª–∏—Å—Ç–∏—á–µ—Å–∫–æ–π "
                                               "—Ä–µ–≤–æ–ª—é—Ü–∏–∏. –°–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –Ω–µ—Ç-–Ω–µ—Ç –Ω–µ –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ,"
                                               " –∞ –¥–µ–ª–∞—Ç—å –∫—Ä–∞—Å–∏–≤–µ–π—à–∏–µ —Ñ–æ—Ç–æ–∫–∞—Ä—Ç–æ—á–∫–∏ —Ä—è–¥–æ–º —Å –í–æ—Å—Ç–æ–∫–æ–º. –ê —á—Ç–æ–±—ã –≤–∞—à–∏"
                                               " –±–∞–ª–ª—ã –±—ã–ª–∏ –∑–∞—Å—á–∏—Ç–∞–Ω—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ —Ñ–æ—Ç–æ –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –≥—Ä—É–ø–ø—ã "
                                               "–í–ö.", reply_markup=men)

    elif "–í–æ—Å—Ç–æ–∫" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("4q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–†–∞–∫–µ—Ç–∞-–Ω–æ—Å–∏—Ç–µ–ª—å ¬´–í–æ—Å—Ç–æ–∫¬ª –∑–∞–Ω–∏–º–∞–µ—Ç –æ—Å–æ–±–æ–µ –º–µ—Å—Ç–æ –≤ –∏—Å—Ç–æ—Ä–∏–∏ –æ—Ç–µ—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–π"
                                               " –∫–æ—Å–º–∏—á–µ—Å–∫–æ–π –ø—Ä–æ–≥—Ä–∞–º–º—ã, –≤–µ–¥—å –∏–º–µ–Ω–Ω–æ —ç—Ç–æ—Ç –∞–ø–ø–∞—Ä–∞—Ç 12 –∞–ø—Ä–µ–ª—è 1961 –≥–æ–¥–∞ "
                                               "–≤—ã–≤–µ–ª –Ω–∞ –æ–∫–æ–ª–æ–∑–µ–º–Ω—É—é –æ—Ä–±–∏—Ç—É ¬´–í–æ—Å—Ç–æ–∫-1¬ª, –Ω–∞ –∫–æ—Ç–æ—Ä–æ–º –Æ—Ä–∏–π –ì–∞–≥–∞—Ä–∏–Ω "
                                               "—Å–æ–≤–µ—Ä—à–∏–ª"
                                               " –ø–µ—Ä–≤—ã–π –≤ –∏—Å—Ç–æ—Ä–∏–∏ –ó–µ–º–ª–∏ –ø–∏–ª–æ—Ç–∏—Ä—É–µ–º—ã–π –ø–æ–ª–µ—Ç –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ. "
                                               "–ú–∞–∫–µ—Ç —Ä–∞–∫–µ—Ç—ã-–Ω–æ—Å–∏—Ç–µ–ª—è ¬´–í–æ—Å—Ç–æ–∫¬ª –±—ã–ª —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –Ω–∞ –í–î–ù–• 8 –∏—é–ª—è 1967 –≥–æ–¥–∞ "
                                               "‚Äî –≤ —á–µ—Å—Ç—å 50-–ª–µ—Ç–∏—è —Å–æ –¥–Ω—è –í–µ–ª–∏–∫–æ–π –û–∫—Ç—è–±—Ä—å—Å–∫–æ–π —Å–æ—Ü–∏–∞–ª–∏—Å—Ç–∏—á–µ—Å–∫–æ–π "
                                               "—Ä–µ–≤–æ–ª—é—Ü–∏–∏. –°–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –Ω–µ—Ç-–Ω–µ—Ç –Ω–µ –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ,"
                                               " –∞ –¥–µ–ª–∞—Ç—å –∫—Ä–∞—Å–∏–≤–µ–π—à–∏–µ —Ñ–æ—Ç–æ–∫–∞—Ä—Ç–æ—á–∫–∏ —Ä—è–¥–æ–º —Å –í–æ—Å—Ç–æ–∫–æ–º. –ê —á—Ç–æ–±—ã –≤–∞—à–∏"
                                               " –±–∞–ª–ª—ã –±—ã–ª–∏ –∑–∞—Å—á–∏—Ç–∞–Ω—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ —Ñ–æ—Ç–æ –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –≥—Ä—É–ø–ø—ã "
                                               "–í–ö.", reply_markup=men)

    elif "–≤–æ—Å—Ç–æ–∫" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("4q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–†–∞–∫–µ—Ç–∞-–Ω–æ—Å–∏—Ç–µ–ª—å ¬´–í–æ—Å—Ç–æ–∫¬ª –∑–∞–Ω–∏–º–∞–µ—Ç –æ—Å–æ–±–æ–µ –º–µ—Å—Ç–æ –≤ –∏—Å—Ç–æ—Ä–∏–∏ –æ—Ç–µ—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–π"
                                               " –∫–æ—Å–º–∏—á–µ—Å–∫–æ–π –ø—Ä–æ–≥—Ä–∞–º–º—ã, –≤–µ–¥—å –∏–º–µ–Ω–Ω–æ —ç—Ç–æ—Ç –∞–ø–ø–∞—Ä–∞—Ç 12 –∞–ø—Ä–µ–ª—è 1961 –≥–æ–¥–∞ "
                                               "–≤—ã–≤–µ–ª –Ω–∞ –æ–∫–æ–ª–æ–∑–µ–º–Ω—É—é –æ—Ä–±–∏—Ç—É ¬´–í–æ—Å—Ç–æ–∫-1¬ª, –Ω–∞ –∫–æ—Ç–æ—Ä–æ–º –Æ—Ä–∏–π –ì–∞–≥–∞—Ä–∏–Ω "
                                               "—Å–æ–≤–µ—Ä—à–∏–ª"
                                               " –ø–µ—Ä–≤—ã–π –≤ –∏—Å—Ç–æ—Ä–∏–∏ –ó–µ–º–ª–∏ –ø–∏–ª–æ—Ç–∏—Ä—É–µ–º—ã–π –ø–æ–ª–µ—Ç –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ. "
                                               "–ú–∞–∫–µ—Ç —Ä–∞–∫–µ—Ç—ã-–Ω–æ—Å–∏—Ç–µ–ª—è ¬´–í–æ—Å—Ç–æ–∫¬ª –±—ã–ª —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –Ω–∞ –í–î–ù–• 8 –∏—é–ª—è 1967 –≥–æ–¥–∞ "
                                               "‚Äî –≤ —á–µ—Å—Ç—å 50-–ª–µ—Ç–∏—è —Å–æ –¥–Ω—è –í–µ–ª–∏–∫–æ–π –û–∫—Ç—è–±—Ä—å—Å–∫–æ–π —Å–æ—Ü–∏–∞–ª–∏—Å—Ç–∏—á–µ—Å–∫–æ–π "
                                               "—Ä–µ–≤–æ–ª—é—Ü–∏–∏. –°–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –Ω–µ—Ç-–Ω–µ—Ç –Ω–µ –≤ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–µ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–æ,"
                                               " –∞ –¥–µ–ª–∞—Ç—å –∫—Ä–∞—Å–∏–≤–µ–π—à–∏–µ —Ñ–æ—Ç–æ–∫–∞—Ä—Ç–æ—á–∫–∏ —Ä—è–¥–æ–º —Å –í–æ—Å—Ç–æ–∫–æ–º. –ê —á—Ç–æ–±—ã –≤–∞—à–∏"
                                               " –±–∞–ª–ª—ã –±—ã–ª–∏ –∑–∞—Å—á–∏—Ç–∞–Ω—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ —Ñ–æ—Ç–æ –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –≥—Ä—É–ø–ø—ã "
                                               "–í–ö.", reply_markup=men)

    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=knop4)


def kno5(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–ê—Ç–æ–º" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("5q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="‚Äú–£—á—ë–Ω—ã–µ —Ä–∞—Å—â–µ–ø–∏–ª–∏ –∞—Ç–æ–º. –¢–µ–ø–µ—Ä—å –∞—Ç–æ–º —Ä–∞—Å—â–µ–ø–ª—è–µ—Ç –Ω–∞—Å‚Äù –ö–≤–µ–Ω—Ç–∏–Ω –†–µ–π–Ω–æ–ª–¥—Å. "
                                               "–ü–∞–≤–∏–ª—å–æ–Ω ‚Ññ71 –≥–¥–µ –≤ 1963 –≥–æ–¥—É —Ä–∞–∑–º–µ—Å—Ç–∏–ª–∞—Å—å —ç–∫—Å–ø–æ–∑–∏—Ü–∏—è ¬´–ê—Ç–æ–º–Ω–∞—è —ç–Ω–µ—Ä–≥–∏—è¬ª."
                                               " –°—Ç–µ–Ω–¥—ã —è–¥–µ—Ä–Ω–æ–π —ç–Ω–µ—Ä–≥–µ—Ç–∏–∫–∏, –∞ —Ç–∞–∫–∂–µ –º–æ–¥–µ–ª–∏ —Ç–µ—Ö–Ω–∏–∫–∏, –∏—Å–ø–æ–ª—å–∑—É–µ–º–æ–π –≤ "
                                               "–æ—Ç—Ä–∞—Å–ª–∏ ¬´–º–∏—Ä–Ω–æ–≥–æ –∞—Ç–æ–º–∞¬ª, –≤ —Ç–æ–º —á–∏—Å–ª–µ –º–∞–∫–µ—Ç—ã –∞—Ç–æ–º–Ω—ã—Ö —Ä–µ–∞–∫—Ç–æ—Ä–æ–≤ –∏ —è–¥–µ—Ä–Ω—ã—Ö "
                                               "—É—Å—Ç–∞–Ω–æ–≤–æ–∫. –õ–∞–π–∫ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—é 18.05.02 ‚ù§ –°–µ–π—á–∞—Å –∂–µ –æ–Ω –ø–∞—Ñ–æ—Å–Ω–æ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è"
                                               " –î–≤–æ—Ä–µ—Ü –ì–æ—Å—É—Å–ª—É–≥, –∏ –µ—Å–ª–∏ —Ç—ã –Ω–µ —Å–ø–µ—à–∏—à—å –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å –±—Ä–∞–∫ –∏–ª–∏ "
                                               "–ø–æ–ª—É—á–∏—Ç—å –Ω–∞—Å–ª–µ–¥—Å—Ç–≤–æ –æ—Ç –¥—è–¥—é—à–∫–∏, —Ç–æ –≤—Ä–µ–º—è –¥–µ–ª–∞—Ç—å –∫—Ä–∞—Å–∏–≤—ã–µ —Ñ–æ—Ç–æ—á–∫–∏."
                                               " –ì–æ–≤–æ—Ä—è—Ç, —á—Ç–æ –∑–∞ –Ω–∏—Ö –µ—â–µ –∏ –±–∞–ª–ª—ã –¥–∞—é—Ç!", reply_markup=men)

    elif "–∞—Ç–æ–º" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("5q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="‚Äú–£—á—ë–Ω—ã–µ —Ä–∞—Å—â–µ–ø–∏–ª–∏ –∞—Ç–æ–º. –¢–µ–ø–µ—Ä—å –∞—Ç–æ–º —Ä–∞—Å—â–µ–ø–ª—è–µ—Ç –Ω–∞—Å‚Äù –ö–≤–µ–Ω—Ç–∏–Ω –†–µ–π–Ω–æ–ª–¥—Å. "
                                               "–ü–∞–≤–∏–ª—å–æ–Ω ‚Ññ71 –≥–¥–µ –≤ 1963 –≥–æ–¥—É —Ä–∞–∑–º–µ—Å—Ç–∏–ª–∞—Å—å —ç–∫—Å–ø–æ–∑–∏—Ü–∏—è ¬´–ê—Ç–æ–º–Ω–∞—è —ç–Ω–µ—Ä–≥–∏—è¬ª."
                                               " –°—Ç–µ–Ω–¥—ã —è–¥–µ—Ä–Ω–æ–π —ç–Ω–µ—Ä–≥–µ—Ç–∏–∫–∏, –∞ —Ç–∞–∫–∂–µ –º–æ–¥–µ–ª–∏ —Ç–µ—Ö–Ω–∏–∫–∏, –∏—Å–ø–æ–ª—å–∑—É–µ–º–æ–π –≤ "
                                               "–æ—Ç—Ä–∞—Å–ª–∏ ¬´–º–∏—Ä–Ω–æ–≥–æ –∞—Ç–æ–º–∞¬ª, –≤ —Ç–æ–º —á–∏—Å–ª–µ –º–∞–∫–µ—Ç—ã –∞—Ç–æ–º–Ω—ã—Ö —Ä–µ–∞–∫—Ç–æ—Ä–æ–≤ –∏ —è–¥–µ—Ä–Ω—ã—Ö "
                                               "—É—Å—Ç–∞–Ω–æ–≤–æ–∫. –õ–∞–π–∫ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—é 18.05.02 ‚ù§\n–°–µ–π—á–∞—Å –∂–µ –æ–Ω –ø–∞—Ñ–æ—Å–Ω–æ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è"
                                               " –î–≤–æ—Ä–µ—Ü –ì–æ—Å—É—Å–ª—É–≥, –∏ –µ—Å–ª–∏ —Ç—ã –Ω–µ —Å–ø–µ—à–∏—à—å –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å –±—Ä–∞–∫ –∏–ª–∏ "
                                               "–ø–æ–ª—É—á–∏—Ç—å –Ω–∞—Å–ª–µ–¥—Å—Ç–≤–æ –æ—Ç –¥—è–¥—é—à–∫–∏, —Ç–æ –≤—Ä–µ–º—è –¥–µ–ª–∞—Ç—å –∫—Ä–∞—Å–∏–≤—ã–µ —Ñ–æ—Ç–æ—á–∫–∏."
                                               " –ì–æ–≤–æ—Ä—è—Ç, —á—Ç–æ –∑–∞ –Ω–∏—Ö –µ—â–µ –∏ –±–∞–ª–ª—ã –¥–∞—é—Ç!", reply_markup=men)

    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=knop5)


def kno6(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–õ–µ–Ω–∏–Ω—É" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        audio = open('otvet.mp3', 'rb')
        bot.send_audio(message.chat.id, audio)
        audio.close()
        f = open("6q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–®–∞–∑–∞–º –Ω–∞ —Å–≤—è–∑–∏ ‚ö°Ô∏è –¢–∞–∫ —Ç—ã —É—á–∏–ª "
                                               "–∏—Å—Ç–æ—Ä–∏—é –∏–ª–∏ –∑–∞–ª–∏–ø–∞–ª –≤ —Ç–∏–∫—Ç–æ–∫–µ ? –í–î–ù–• ‚Äî –Ω–∞—Å—Ç–æ–ª—å–∫–æ –∑–∞–≥–∞–¥–æ—á–Ω–æ–µ –º–µ—Å—Ç–æ,"
                                               " —á—Ç–æ –∑–∞ –≤—Å–µ–º–∏ —Ç–∞–π–Ω–∞–º–∏ –Ω–µ —É—Å–ª–µ–¥–∏—à—å. –ù–æ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–µ–≥–µ–Ω–¥–∞, —è–∫–æ–±—ã –æ–¥–∏–Ω –∏–∑ "
                                               "–≤—ã—Ö–æ–¥–æ–≤ –±—É–Ω–∫–µ—Ä–∞, —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–Ω–æ–≥–æ –ø–µ—Ä–µ–¥ –¥–æ–º–æ–º –¥—Ä—É–∂–±—ã –Ω–∞—Ä–æ–¥–æ–≤, "
                                               "–∑–∞–∫–∞–Ω—á–∏–≤–∞–µ—Ç—Å—è –ø—Ä—è–º–æ –ø–æ–¥ —Å–∫—É–ª—å–ø—Ç—É—Ä–æ–π –õ–µ–Ω–∏–Ω–∞, —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–Ω–æ–π –ø–µ—Ä–µ–¥ "
                                               "–ø–∞–≤–∏–ª—å–æ–Ω–æ–º. –¢—ã –∑–Ω–∞–µ—à—å, —á—Ç–æ –¥–µ–ª–∞—Ç—å, —Å–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –∑–∞ –±–∞–ª–ª–∞–º–∏ "
                                               "–∏–º–µ–Ω–Ω–æ —Ç—É–¥–∞ –∫ –ª–µ–≥–µ–Ω–¥–∞—Ä–Ω–æ–º—É –≤–æ–∂–¥—é! ", reply_markup=men)

    elif "–ª–µ–Ω–∏–Ω—É" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        audio = open('otvet.mp3', 'rb')
        bot.send_audio(message.chat.id, audio)
        audio.close()
        f = open("6q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–®–∞–∑–∞–º –Ω–∞ —Å–≤—è–∑–∏ ‚ö°Ô∏è –¢–∞–∫ —Ç—ã —É—á–∏–ª "
                                               "–∏—Å—Ç–æ—Ä–∏—é –∏–ª–∏ –∑–∞–ª–∏–ø–∞–ª –≤ —Ç–∏–∫—Ç–æ–∫–µ ? –í–î–ù–• ‚Äî –Ω–∞—Å—Ç–æ–ª—å–∫–æ –∑–∞–≥–∞–¥–æ—á–Ω–æ–µ –º–µ—Å—Ç–æ,"
                                               " —á—Ç–æ –∑–∞ –≤—Å–µ–º–∏ —Ç–∞–π–Ω–∞–º–∏ –Ω–µ —É—Å–ª–µ–¥–∏—à—å. –ù–æ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–µ–≥–µ–Ω–¥–∞, —è–∫–æ–±—ã –æ–¥–∏–Ω –∏–∑ "
                                               "–≤—ã—Ö–æ–¥–æ–≤ –±—É–Ω–∫–µ—Ä–∞, —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–Ω–æ–≥–æ –ø–µ—Ä–µ–¥ –¥–æ–º–æ–º –¥—Ä—É–∂–±—ã –Ω–∞—Ä–æ–¥–æ–≤, "
                                               "–∑–∞–∫–∞–Ω—á–∏–≤–∞–µ—Ç—Å—è –ø—Ä—è–º–æ –ø–æ–¥ —Å–∫—É–ª—å–ø—Ç—É—Ä–æ–π –õ–µ–Ω–∏–Ω–∞, —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–Ω–æ–π –ø–µ—Ä–µ–¥ "
                                               "–ø–∞–≤–∏–ª—å–æ–Ω–æ–º. –¢—ã –∑–Ω–∞–µ—à—å, —á—Ç–æ –¥–µ–ª–∞—Ç—å, —Å–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –∑–∞ –±–∞–ª–ª–∞–º–∏ "
                                               "–∏–º–µ–Ω–Ω–æ —Ç—É–¥–∞ –∫ –ª–µ–≥–µ–Ω–¥–∞—Ä–Ω–æ–º—É –≤–æ–∂–¥—é! ", reply_markup=men)

    elif "–õ–µ–Ω–∏–Ω" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        audio = open('otvet.mp3', 'rb')
        bot.send_audio(message.chat.id, audio)
        audio.close()
        f = open("6q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–®–∞–∑–∞–º –Ω–∞ —Å–≤—è–∑–∏ ‚ö°Ô∏è –¢–∞–∫ —Ç—ã —É—á–∏–ª "
                                               "–∏—Å—Ç–æ—Ä–∏—é –∏–ª–∏ –∑–∞–ª–∏–ø–∞–ª –≤ —Ç–∏–∫—Ç–æ–∫–µ ? –í–î–ù–• ‚Äî –Ω–∞—Å—Ç–æ–ª—å–∫–æ –∑–∞–≥–∞–¥–æ—á–Ω–æ–µ –º–µ—Å—Ç–æ,"
                                               " —á—Ç–æ –∑–∞ –≤—Å–µ–º–∏ —Ç–∞–π–Ω–∞–º–∏ –Ω–µ —É—Å–ª–µ–¥–∏—à—å. –ù–æ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–µ–≥–µ–Ω–¥–∞, —è–∫–æ–±—ã –æ–¥–∏–Ω –∏–∑ "
                                               "–≤—ã—Ö–æ–¥–æ–≤ –±—É–Ω–∫–µ—Ä–∞, —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–Ω–æ–≥–æ –ø–µ—Ä–µ–¥ –¥–æ–º–æ–º –¥—Ä—É–∂–±—ã –Ω–∞—Ä–æ–¥–æ–≤, "
                                               "–∑–∞–∫–∞–Ω—á–∏–≤–∞–µ—Ç—Å—è –ø—Ä—è–º–æ –ø–æ–¥ —Å–∫—É–ª—å–ø—Ç—É—Ä–æ–π –õ–µ–Ω–∏–Ω–∞, —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–Ω–æ–π –ø–µ—Ä–µ–¥ "
                                               "–ø–∞–≤–∏–ª—å–æ–Ω–æ–º. –¢—ã –∑–Ω–∞–µ—à—å, —á—Ç–æ –¥–µ–ª–∞—Ç—å, —Å–∫–æ—Ä–µ–µ –æ—Ç–ø—Ä–∞–≤–ª—è–π—Ç–µ—Å—å –∑–∞ –±–∞–ª–ª–∞–º–∏ "
                                               "–∏–º–µ–Ω–Ω–æ —Ç—É–¥–∞ –∫ –ª–µ–≥–µ–Ω–¥–∞—Ä–Ω–æ–º—É –≤–æ–∂–¥—é! ", reply_markup=men)

    elif "–ª–µ–Ω–∏–Ω" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        audio = open('otvet.mp3', 'rb')
        bot.send_audio(message.chat.id, audio)
