    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=knop15)


@bot.callback_query_handler(func=lambda call: True)
def query_handler(call):
    if call.data == 'test_1':
        bot.send_message(call.message.chat.id, '–ù—É–ª–µ–≤–∞—è, —Å—Ç–∞—Ä—Ç–æ–≤–∞—è –∑–∞–≥–∞–¥–∫–∞, —á—Ç–æ–±—ã —Ä–∞–∑–º—è—Ç—å –º–æ–∑–≥–∏.')
        time.sleep(1)
        bot.send_message(call.message.chat.id, '*–í–°–•–í, –í–í–¶, —É —ç—Ç–æ–≥–æ –º–µ—Å—Ç–∞ –º–Ω–æ–≥–æ –Ω–∞–∑–≤–∞–Ω–∏–π, –Ω–æ –Ω–∞ —Å–ª—É—Ö—É –æ–¥–Ω–æ -*', parse_mode="Markdown")
        time.sleep(1)
        message = bot.send_message(call.message.chat.id, text='–ù–∞–ø–∏—à–∏ —Å–≤–æ–π –æ—Ç–≤–µ—Ç –∏ –æ—Ç–ø—Ä–∞–≤—å –µ–≥–æ –º–Ω–µ!')
        bot.register_next_step_handler(message, test1)

    elif call.data == 'test_1_res':
        message = bot.send_message(call.message.chat.id, text='–ù–∞–ø–∏—à–∏ —Å–≤–æ–π –æ—Ç–≤–µ—Ç –∏ –æ—Ç–ø—Ä–∞–≤—å –µ–≥–æ –º–Ω–µ!')
        bot.register_next_step_handler(message, test1)

    elif call.data == 'otpfoto':
        message = bot.send_message(call.message.chat.id, text='–ñ–¥—É —Ñ–æ—Ç–∫—É')
        bot.register_next_step_handler(message, fotoo)

    elif call.data == 'otpfoto_res':
        message = bot.send_message(call.message.chat.id, text='–ñ–¥—É —Ñ–æ—Ç–∫—É')
        bot.register_next_step_handler(message, fotoo)

    elif call.data == 'menu':
        message = bot.send_message(call.message.chat.id, text='–í–æ—Ç —Ç–≤–æ—ë –º–µ–Ω—é:', reply_markup=osnov_menu)

