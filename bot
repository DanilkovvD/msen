        bot.send_message(message.chat.id, neprav, reply_markup=knop8)


def kno9(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–§–æ–Ω—Ç–∞–Ω" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("9q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ —Å–ª–æ–∂–Ω–æ, –Ω–æ –≤—ã —Å–ø—Ä–∞–≤–∏–ª–∏—Å—å! "
                                               "–§–æ–Ω—Ç–∞–Ω ¬´–ö–∞–º–µ–Ω–Ω—ã–π —Ü–≤–µ—Ç–æ–∫¬ª ‚Äî –æ–¥–∏–Ω –∏–∑ —á–µ—Ç—ã—Ä–µ—Ö –∑–Ω–∞–º–µ–Ω–∏—Ç—ã—Ö "
                                               "–∏—Å—Ç–æ—Ä–∏—á–µ—Å–∫–∏—Ö —Ñ–æ–Ω—Ç–∞–Ω–æ–≤ –í–î–ù–• —É–∂–µ —Ä–∞—Å—Ü–≤—ë–ª –∏ –∂–¥—ë—Ç –≤–∞—Å!", reply_markup=men)

    elif "—Ñ–æ–Ω—Ç–∞–Ω" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("9q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ —Å–ª–æ–∂–Ω–æ, –Ω–æ –≤—ã —Å–ø—Ä–∞–≤–∏–ª–∏—Å—å! "
                                               "–§–æ–Ω—Ç–∞–Ω ¬´–ö–∞–º–µ–Ω–Ω—ã–π —Ü–≤–µ—Ç–æ–∫¬ª ‚Äî –æ–¥–∏–Ω –∏–∑ —á–µ—Ç—ã—Ä–µ—Ö –∑–Ω–∞–º–µ–Ω–∏—Ç—ã—Ö "
                                               "–∏—Å—Ç–æ—Ä–∏—á–µ—Å–∫–∏—Ö —Ñ–æ–Ω—Ç–∞–Ω–æ–≤ –í–î–ù–• —É–∂–µ —Ä–∞—Å—Ü–≤—ë–ª –∏ –∂–¥—ë—Ç –≤–∞—Å!", reply_markup=men)
    elif "–ö–∞–º–µ–Ω–Ω—ã–π" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("9q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ —Å–ª–æ–∂–Ω–æ, –Ω–æ –≤—ã —Å–ø—Ä–∞–≤–∏–ª–∏—Å—å! "
                                               "–§–æ–Ω—Ç–∞–Ω ¬´–ö–∞–º–µ–Ω–Ω—ã–π —Ü–≤–µ—Ç–æ–∫¬ª ‚Äî –æ–¥–∏–Ω –∏–∑ —á–µ—Ç—ã—Ä–µ—Ö –∑–Ω–∞–º–µ–Ω–∏—Ç—ã—Ö "
                                               "–∏—Å—Ç–æ—Ä–∏—á–µ—Å–∫–∏—Ö —Ñ–æ–Ω—Ç–∞–Ω–æ–≤ –í–î–ù–• —É–∂–µ —Ä–∞—Å—Ü–≤—ë–ª –∏ –∂–¥—ë—Ç –≤–∞—Å!", reply_markup=men)

    elif "–∫–∞–º–µ–Ω–Ω—ã–π" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("9q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ —Å–ª–æ–∂–Ω–æ, –Ω–æ –≤—ã —Å–ø—Ä–∞–≤–∏–ª–∏—Å—å! "
                                               "–§–æ–Ω—Ç–∞–Ω ¬´–ö–∞–º–µ–Ω–Ω—ã–π —Ü–≤–µ—Ç–æ–∫¬ª ‚Äî –æ–¥–∏–Ω –∏–∑ —á–µ—Ç—ã—Ä–µ—Ö –∑–Ω–∞–º–µ–Ω–∏—Ç—ã—Ö "
                                               "–∏—Å—Ç–æ—Ä–∏—á–µ—Å–∫–∏—Ö —Ñ–æ–Ω—Ç–∞–Ω–æ–≤ –í–î–ù–• —É–∂–µ —Ä–∞—Å—Ü–≤—ë–ª –∏ –∂–¥—ë—Ç –≤–∞—Å!", reply_markup=men)

    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=knop9)


def kno10(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–§–ª–æ—Ä–∞" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("10q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–ë–∏–Ω–≥–æ üßöüèªüßöüèªüßöüèª –ö—Å—Ç–∞—Ç–∏ –§–ª–æ—Ä–∞ —ç—Ç–æ –Ω–µ —Ç–æ–ª—å–∫–æ —Ñ–µ–µ—á–∫–∞ –í–∏–Ω–∫—Å,"
                                               " –Ω–æ –µ—â—ë –∏ –¥–µ–∫–æ—Ä–∞—Ç–∏–≤–Ω–∞—è –∫–æ–º–ø–æ–∑–∏—Ü–∏—è –Ω–∞ —Ñ–∞—Å–∞–¥–µ –ø–∞–≤–∏–ª—å–æ–Ω–∞ –¶–≤–µ—Ç–æ–≤–æ–¥—Å—Ç–≤–æ. "
                                               "–û–±—è–∑–∞—Ç–µ–ª—å–Ω–æ —Å–¥–µ–ª–∞–π—Ç–µ –∫–æ–º–∞–Ω–¥–Ω–æ–µ —Ñ–æ—Ç–æ –≤–æ–∑–ª–µ –Ω–µ—ë –≤ –ø–∞–º—è—Ç—å –æ–± —ç—Ç–æ–º —á—É–¥–µ—Å–Ω–æ–º"
                                               " –¥–Ω–µ!", reply_markup=men)

    elif "—Ñ–ª–æ—Ä–∞" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID,
                         text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("10q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–ë–∏–Ω–≥–æ üßöüèªüßöüèªüßöüèª –ö—Å—Ç–∞—Ç–∏ –§–ª–æ—Ä–∞ —ç—Ç–æ –Ω–µ —Ç–æ–ª—å–∫–æ —Ñ–µ–µ—á–∫–∞ –í–∏–Ω–∫—Å,"
                                               " –Ω–æ –µ—â—ë –∏ –¥–µ–∫–æ—Ä–∞—Ç–∏–≤–Ω–∞—è –∫–æ–º–ø–æ–∑–∏—Ü–∏—è –Ω–∞ —Ñ–∞—Å–∞–¥–µ –ø–∞–≤–∏–ª—å–æ–Ω–∞ –¶–≤–µ—Ç–æ–≤–æ–¥—Å—Ç–≤–æ. "
                                               "–û–±—è–∑–∞—Ç–µ–ª—å–Ω–æ —Å–¥–µ–ª–∞–π—Ç–µ –∫–æ–º–∞–Ω–¥–Ω–æ–µ —Ñ–æ—Ç–æ –≤–æ–∑–ª–µ –Ω–µ—ë –≤ –ø–∞–º—è—Ç—å –æ–± —ç—Ç–æ–º —á—É–¥–µ—Å–Ω–æ–º"
                                               " –¥–Ω–µ!", reply_markup=men)

    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=knop10)


