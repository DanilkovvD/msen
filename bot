        bot.send_message(message.chat.id, neprav, reply_markup=knop6)


def kno7(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–ü–∏—Ä–æ–ª–∏–∑" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("7q.PNG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–í—Å—ë —Ç–∞–∫! –ù–æ —Å–µ–≥–æ–¥–Ω—è –Ω–∞—Å –∏–Ω—Ç–µ—Ä–µ—Å—É–µ—Ç –Ω–µ –ø—Ä–æ—Å—Ç–æ –ø–∏—Ä–æ–ª–∏–∑, "
                                               "–∞ –∏–º–µ–Ω–Ω–æ –ø–∏—Ä–æ–ª–∏–∑ –Ω–µ—Ñ—Ç–∏. –û–Ω —è–≤–ª—è–µ—Ç—Å—è –æ—Ç–ª–∏—á–Ω—ã–º —Å–ø–æ—Å–æ–±–æ–º –±–æ—Ä—å–±—ã"
                                               " –≤ —Å–ª—É—á–∞—è—Ö —Ä–∞–∑–ª–∏–≤–∞ –Ω–µ—Ñ—Ç–∏ –∏ –Ω–µ—Ñ—Ç—è–Ω–æ–≥–æ —à–ª–∞–º–∞. –ê –≤—ã –∑–Ω–∞–µ—Ç–µ, —á—Ç–æ –Ω–∞ "
                                               "–í–î–ù–• –µ—Å—Ç—å —Ü–µ–ª—ã–π –º—É–∑–µ–π –Ω–µ—Ñ—Ç–∏ ? –ù–æ –¥–∞–∂–µ —Ö–∏–º–∏–∫–∞–º –∏–Ω–æ–≥–¥–∞ –Ω—É–∂–Ω–æ –æ—Ç–¥—ã—Ö–∞—Ç—å, "
                                               "–ø–æ—ç—Ç–æ–º—É –ø–µ—Ä–µ–¥ –Ω–∏–º —Ä–∞—Å–ø–æ–ª–∞–≥–∞–µ—Ç—Å—è –Ω–∞–∏–∫—Ä—É—Ç–µ–π—à–∞—è –¥–µ—Ç—Å–∫–∞—è –∏–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω–∞—è "
                                               "–ø–ª–æ—â–∞–¥–∫–∞ –æ—Ç –õ—É–∫–æ–π–ª, —Ç–∞–º –≤—ã —Å–º–æ–∂–µ—Ç–µ –∏–∑—É—á–∏—Ç—å –ø—Ä–∏–Ω—Ü–∏–ø —Ä–∞–±–æ—Ç—ã"
                                               " –Ω–µ—Ñ—Ç–µ–ø–µ—Ä–µ—Ä–∞–±–∞—Ç—ã–≤–∞—é—â–µ–≥–æ –∫–æ–º–ø–ª–µ–∫—Å–∞ —Å –ø–æ–º–æ—â—å—é —á–µ—Ç—ã—Ä–µ—Ö –∏–≥—Ä–æ–≤—ã—Ö –º–æ–¥—É–ª–µ–π"
                                               " —Å –≥–æ—Ä–∫–∞–º–∏, —Å–∫–∞–ª–æ–¥—Ä–æ–º–∞–º–∏, –±–∞—Ç—É—Ç–∞–º–∏, –∫–∞—á–µ–ª—è–º–∏ –∏ –∏–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω—ã–º–∏ –ø–∞–Ω–µ–ª—è–º–∏"
                                               " üòèüòù. –¢–µ–ø–µ—Ä—å –≤—ã –∑–Ω–∞–µ—Ç–µ, –∑–∞ –æ—Ç–¥—ã—Ö–æ–º –∏ —Ñ–æ—Ç–æ—á–∫–∞–º–∏ ‚Äì –Ω–∞"
                                               " –ø–ª–æ—â–∞–¥–∫—É!", reply_markup=men)

    elif "–ø–∏—Ä–æ–ª–∏–∑" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("7q.PNG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–í—Å—ë —Ç–∞–∫! –ù–æ —Å–µ–≥–æ–¥–Ω—è –Ω–∞—Å –∏–Ω—Ç–µ—Ä–µ—Å—É–µ—Ç –Ω–µ –ø—Ä–æ—Å—Ç–æ –ø–∏—Ä–æ–ª–∏–∑, "
                                               "–∞ –∏–º–µ–Ω–Ω–æ –ø–∏—Ä–æ–ª–∏–∑ –Ω–µ—Ñ—Ç–∏. –û–Ω —è–≤–ª—è–µ—Ç—Å—è –æ—Ç–ª–∏—á–Ω—ã–º —Å–ø–æ—Å–æ–±–æ–º –±–æ—Ä—å–±—ã"
                                               " –≤ —Å–ª—É—á–∞—è—Ö —Ä–∞–∑–ª–∏–≤–∞ –Ω–µ—Ñ—Ç–∏ –∏ –Ω–µ—Ñ—Ç—è–Ω–æ–≥–æ —à–ª–∞–º–∞. –ê –≤—ã –∑–Ω–∞–µ—Ç–µ, —á—Ç–æ –Ω–∞ "
                                               "–í–î–ù–• –µ—Å—Ç—å —Ü–µ–ª—ã–π –º—É–∑–µ–π –Ω–µ—Ñ—Ç–∏ ? –ù–æ –¥–∞–∂–µ —Ö–∏–º–∏–∫–∞–º –∏–Ω–æ–≥–¥–∞ –Ω—É–∂–Ω–æ –æ—Ç–¥—ã—Ö–∞—Ç—å, "
                                               "–ø–æ—ç—Ç–æ–º—É –ø–µ—Ä–µ–¥ –Ω–∏–º —Ä–∞—Å–ø–æ–ª–∞–≥–∞–µ—Ç—Å—è –Ω–∞–∏–∫—Ä—É—Ç–µ–π—à–∞—è –¥–µ—Ç—Å–∫–∞—è –∏–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω–∞—è "
                                               "–ø–ª–æ—â–∞–¥–∫–∞ –æ—Ç –õ—É–∫–æ–π–ª, —Ç–∞–º –≤—ã —Å–º–æ–∂–µ—Ç–µ –∏–∑—É—á–∏—Ç—å –ø—Ä–∏–Ω—Ü–∏–ø —Ä–∞–±–æ—Ç—ã"
                                               " –Ω–µ—Ñ—Ç–µ–ø–µ—Ä–µ—Ä–∞–±–∞—Ç—ã–≤–∞—é—â–µ–≥–æ –∫–æ–º–ø–ª–µ–∫—Å–∞ —Å –ø–æ–º–æ—â—å—é —á–µ—Ç—ã—Ä–µ—Ö –∏–≥—Ä–æ–≤—ã—Ö –º–æ–¥—É–ª–µ–π"
                                               " —Å –≥–æ—Ä–∫–∞–º–∏, —Å–∫–∞–ª–æ–¥—Ä–æ–º–∞–º–∏, –±–∞—Ç—É—Ç–∞–º–∏, –∫–∞—á–µ–ª—è–º–∏ –∏ –∏–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω—ã–º–∏ –ø–∞–Ω–µ–ª—è–º–∏"
                                               " üòèüòù. –¢–µ–ø–µ—Ä—å –≤—ã –∑–Ω–∞–µ—Ç–µ, –∑–∞ –æ—Ç–¥—ã—Ö–æ–º –∏ —Ñ–æ—Ç–æ—á–∫–∞–º–∏ ‚Äì –Ω–∞"
                                               " –ø–ª–æ—â–∞–¥–∫—É!", reply_markup=men)

    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=knop7)


def kno8(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–§–æ–Ω—Ç–∞–Ω" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
