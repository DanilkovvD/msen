        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("15q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–ö–∞–∫–∏–µ –≤—ã —É–º–Ω–∏—á–∫–∏ ‚ù§Ô∏è –ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º, –≤—ã "
                                               "–æ—Ç–≥–∞–¥–∞–ª–∏ –ø–æ—Å–ª–µ–¥–Ω—é—é –∑–∞–≥–∞–¥–∫—É! –ù–∞ –í–î–ù–• –Ω–∞—Ö–æ–¥–∏—Ç—Å—è —Ñ–æ–Ω—Ç–∞–Ω "
                                               "–î—Ä—É–∂–±—ã –ù–∞—Ä–æ–¥–æ–≤, –ø–æ—Ä–∞–∂–∞—é—â–µ–π —Å–≤–æ–µ–π –∫—Ä–∞—Å–æ—Ç–æ–π. –ü–æ –ª–µ–≥–µ–Ω–¥–µ,"
                                               "  ¬´–æ–±–ª–∞—á–∏—Ç—å¬ª –¥–µ–≤—É—à–µ–∫ –≤ –∏—Å–∫—É—Å–Ω—ã–π –Ω–∞—Ä—è–¥ –∏–∑ –¥—Ä–∞–≥–æ—Ü–µ–Ω–Ω–æ–≥–æ –º–µ—Ç–∞–ª–ª–∞"
                                               " –ø—Ä–∏–∫–∞–∑–∞–ª —Å–∞–º –°—Ç–∞–ª–∏–Ω: ¬´–ß—Ç–æ –∂ —É –Ω–∞—Å, –Ω–∞ –¥–µ–≤ –Ω–∞—à–∏—Ö –∏ –∑–æ–ª–æ—Ç–∞ –Ω–µ"
                                               " –Ω–∞–π–¥–µ—Ç—Å—è?¬ª. –ù–µ –∑–∞–±—É–¥—å—Ç–µ —Å–¥–µ–ª–∞—Ç—å –∫–æ–º–∞–Ω–¥–Ω–æ–µ —Ñ–æ—Ç–æ –≤ "
                                               "—ç—Ç–æ–π –ª–æ–∫–∞—Ü–∏–∏.", reply_markup=men)

    elif "–≤–æ–¥–∫–∞" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("15q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–ö–∞–∫–∏–µ –≤—ã —É–º–Ω–∏—á–∫–∏ ‚ù§Ô∏è –ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º, –≤—ã "
                                               "–æ—Ç–≥–∞–¥–∞–ª–∏ –ø–æ—Å–ª–µ–¥–Ω—é—é –∑–∞–≥–∞–¥–∫—É! –ù–∞ –í–î–ù–• –Ω–∞—Ö–æ–¥–∏—Ç—Å—è —Ñ–æ–Ω—Ç–∞–Ω "
                                               "–î—Ä—É–∂–±—ã –ù–∞—Ä–æ–¥–æ–≤, –ø–æ—Ä–∞–∂–∞—é—â–µ–π —Å–≤–æ–µ–π –∫—Ä–∞—Å–æ—Ç–æ–π. –ü–æ –ª–µ–≥–µ–Ω–¥–µ,"
                                               "  ¬´–æ–±–ª–∞—á–∏—Ç—å¬ª –¥–µ–≤—É—à–µ–∫ –≤ –∏—Å–∫—É—Å–Ω—ã–π –Ω–∞—Ä—è–¥ –∏–∑ –¥—Ä–∞–≥–æ—Ü–µ–Ω–Ω–æ–≥–æ –º–µ—Ç–∞–ª–ª–∞"
                                               " –ø—Ä–∏–∫–∞–∑–∞–ª —Å–∞–º –°—Ç–∞–ª–∏–Ω: ¬´–ß—Ç–æ –∂ —É –Ω–∞—Å, –Ω–∞ –¥–µ–≤ –Ω–∞—à–∏—Ö –∏ –∑–æ–ª–æ—Ç–∞ –Ω–µ"
                                               " –Ω–∞–π–¥–µ—Ç—Å—è?¬ª. –ù–µ –∑–∞–±—É–¥—å—Ç–µ —Å–¥–µ–ª–∞—Ç—å –∫–æ–º–∞–Ω–¥–Ω–æ–µ —Ñ–æ—Ç–æ –≤ "
