@bot.message_handler(commands=["start"])
def start_msg(message):
    bot.send_message(message.chat.id, text="–ü—Ä–∏–≤–µ—Ç, " + message.from_user.first_name + ", –≤–æ—Ç –º—ã –∏ –≤—Å—Ç—Ä–µ—Ç–∏–ª–∏—Å—å!"
                                                                                       "\n–ù–∞–¥–µ–µ–º—Å—è, —á—Ç–æ –Ω–∞—à–µ –º–∞–ª–µ–Ω—å–∫–æ–µ"
                                                                                       " –ø—Ä–∏–∫–ª—é—á–µ–Ω–∏–µ –Ω–µ –ø–æ–∫–∞–∂–µ—Ç—Å—è –≤–∞–º "
                                                                                       "—Å–ª–æ–∂–Ω—ã–º."
                                                                                       "\n\n–ù—É —á—Ç–æ, –≤–∞—à–∞ –∫–æ–º–∞–Ω–¥–∞ –∑–¥–µ—Å—å,"
                                                                                       " –∞ —ç—Ç–æ –∑–Ω–∞—á–∏—Ç, —á—Ç–æ –≤—ã –≥–æ—Ç–æ–≤—ã "
                                                                                       "–æ—Ç–≥–∞–¥—ã–≤–∞—Ç—å –∑–∞–≥–∞–¥–∫–∏. "
                                                                                       "–ù–∞—á–Ω—ë–º??", reply_markup=test_menu)


def test1(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–í–î–ù–•" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ –Ω–µ—Å–ª–æ–∂–Ω–æ, –ø—Ä–∞–≤–¥–∞? –ê —Ç–µ–ø–µ—Ä—å –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Ñ–æ—Ç–æ –≤—Å–µ–π –∫–æ–º–∞–Ω–¥—ã –≤ —ç—Ç–æ–π –ª–æ–∫–∞—Ü–∏–∏, —á—Ç–æ–±—ã –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç –±—ã–ª –∑–∞—Å—á–∏—Ç–∞–Ω.", reply_markup=foto)
    elif "–≤–¥–Ω—Ö" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ –Ω–µ—Å–ª–æ–∂–Ω–æ, –ø—Ä–∞–≤–¥–∞? –ê —Ç–µ–ø–µ—Ä—å –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Ñ–æ—Ç–æ –≤—Å–µ–π –∫–æ–º–∞–Ω–¥—ã –≤ —ç—Ç–æ–π –ª–æ–∫–∞—Ü–∏–∏, —á—Ç–æ–±—ã –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç –±—ã–ª –∑–∞—Å—á–∏—Ç–∞–Ω.", reply_markup=foto)
    elif "–í–¥–Ω—Ö" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ –Ω–µ—Å–ª–æ–∂–Ω–æ, –ø—Ä–∞–≤–¥–∞? –ê —Ç–µ–ø–µ—Ä—å –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Ñ–æ—Ç–æ –≤—Å–µ–π –∫–æ–º–∞–Ω–¥—ã –≤ —ç—Ç–æ–π –ª–æ–∫–∞—Ü–∏–∏, —á—Ç–æ–±—ã –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç –±—ã–ª –∑–∞—Å—á–∏—Ç–∞–Ω.", reply_markup=foto)
    elif "–¥–æ—Å—Ç–∏–∂–µ–Ω–∏–π" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ –Ω–µ—Å–ª–æ–∂–Ω–æ, –ø—Ä–∞–≤–¥–∞? –ê —Ç–µ–ø–µ—Ä—å –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Ñ–æ—Ç–æ –≤—Å–µ–π –∫–æ–º–∞–Ω–¥—ã –≤ —ç—Ç–æ–π –ª–æ–∫–∞—Ü–∏–∏, —á—Ç–æ–±—ã –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç –±—ã–ª –∑–∞—Å—á–∏—Ç–∞–Ω.", reply_markup=foto)
    elif "–î–æ—Å—Ç–∏–∂–µ–Ω–∏–π" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, text="–≠—Ç–æ –±—ã–ª–æ –Ω–µ—Å–ª–æ–∂–Ω–æ, –ø—Ä–∞–≤–¥–∞? –ê —Ç–µ–ø–µ—Ä—å –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Ñ–æ—Ç–æ –≤—Å–µ–π –∫–æ–º–∞–Ω–¥—ã –≤ —ç—Ç–æ–π –ª–æ–∫–∞—Ü–∏–∏, —á—Ç–æ–±—ã –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç –±—ã–ª –∑–∞—Å—á–∏—Ç–∞–Ω.", reply_markup=foto)
    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, neprav, reply_markup=restart)


def fotoo(message):
    if message.photo:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, 'Beautiful 1000000000/10 ! –ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º, –≤—ã –ø–æ–ª—É—á–∞–µ—Ç–µ —Å–≤–æ–∏ –ø–µ—Ä–≤—ã–µ –±–∞–ª–ª—ã. –î–∞–≤–∞–π—Ç–µ –¥–≤–∏–≥–∞—Ç—å—Å—è –¥–∞–ª—å—à–µ!')
        time.sleep(2)
        bot.send_message(message.chat.id, '–≠—Ç–æ ‚Äî –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é, –≤ –∫–æ—Ç–æ—Ä–æ–º –Ω–∞—Ö–æ–¥—è—Ç—Å—è –≤—Å–µ –∑–∞–≥–∞–¥–∫–∏. –í—ã –º–æ–∂–µ—Ç–µ –æ—Ç–≥–∞–¥—ã–≤–∞—Ç—å –∏—Ö –≤ –ª—é–±–æ–π –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏. –£–¥–∞—á–∏!', reply_markup=osnov_menu)
    else:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüî¥üî¥üî¥ " + message.from_user.first_name + ' –æ—à–∏–±—Å—è! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        bot.send_message(message.chat.id, '–ö–∞–∂–µ—Ç—Å—è, —ç—Ç–æ –Ω–µ —Ñ–æ—Ç–æ, –ø–æ–ø—Ä–æ–±—É–π –µ—â—ë —Ä–∞–∑', reply_markup=restart_f)


def kno1(message):
    proverka = message.text
    string_cp = proverka.replace(',', '')
    dod = string_cp.split()
    no_otvet = ['–£–ø—Å, –Ω–µ–≤–µ—Ä–Ω–æ', '–≠—Ç–æ –≤—Å–µ –∫–æ–Ω–µ—á–Ω–æ –∫—Ä—É—Ç–æ, –Ω–æ –¥–∞–≤–∞–π –ø–æ–¥—É–º–∞–µ–º –µ—â–µ —Ä–∞–∑', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑',
                '–¢—è—É - —Ç—è—É - —Ç—è—É –Ω—É –Ω–∏—á–µ–≥–æ —Å—Ç—Ä–∞—à–Ω–æ–≥–æ, –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–≤–µ–∑—ë—Ç', '–ü–æ–∏—â–∏—Ç–µ –µ—â—ë –≤–∞—Ä–∏–∞–Ω—Ç—ã',
                '–•–æ—Ä–æ—à–∏–π –æ—Ç–≤–µ—Ç –Ω–∞ –∑–∞–≥–∞–¥–∫—É –≤ –∫–∞–∫–æ–º-–Ω–∏–±—É–¥—å –¥—Ä—É–≥–æ–º –∫–≤–µ—Å—Ç–µ',
                'Error 404. –î–ª—è –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç',
                '–ì–æ–≤–æ—Ä—è—Ç –ú–µ–Ω–¥–µ–ª–µ–µ–≤ –∏–∑–æ–±—Ä—ë–ª –≤–æ–¥–∫—É, –Ω–æ —ç—Ç–æ –æ—à–∏–±–∫–∞, —Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –∏ –≤–∞—à –æ—Ç–≤–µ—Ç']

    neprav = random.choice(no_otvet)
    if "–ë–µ–ª–∞—Ä—É—Å—å" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("1q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ—Ç –∑–Ω–∞–º–µ–Ω–∏—Ç—ã–π –±–µ–ª–æ—Ä—É—Å—Å–∫–∏–π —Ç—Ä–∏–∫–æ—Ç–∞–∂, —Ç—ã –µ–≥–æ —É–∑–Ω–∞–µ—à—å –∏–∑ —Ç—ã—Å—è—á–∏ Calvin Klein."
                                               " –ê —Ç—ã –∑–Ω–∞–µ—à—å, —á—Ç–æ –Ω–∞ –í–î–ù–• –µ—Å—Ç—å —Ü–µ–ª—ã–π –ø–∞–≤–∏–ª—å–æ–Ω –≤ —á–µ—Å—Ç—å —ç—Ç–æ–π —Ä–µ—Å–ø—É–±–ª–∏–∫–∏? "
                                               "\n–ß—Ç–æ–±—ã –ø–æ–ª—É—á–∏—Ç—å –±–∞–ª–ª—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Å—è –∫ –ø–∞–≤–∏–ª—å–æ–Ω—É ‚Ññ18 –∏ –ø—Ä–∏—Å—ã–ª–∞–π —Ñ–æ—Ç–æ "
                                               "–∫–æ–º–∞–Ω–¥—ã –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –Ω–∞—à–µ–π –≥—Ä—É–ø–ø—ã –í–ö.", reply_markup=men)
    elif "–±–µ–ª–∞—Ä—É—Å—å" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("1q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ—Ç –∑–Ω–∞–º–µ–Ω–∏—Ç—ã–π –±–µ–ª–æ—Ä—É—Å—Å–∫–∏–π —Ç—Ä–∏–∫–æ—Ç–∞–∂, —Ç—ã –µ–≥–æ —É–∑–Ω–∞–µ—à—å –∏–∑ —Ç—ã—Å—è—á–∏ Calvin Klein."
                                               " –ê —Ç—ã –∑–Ω–∞–µ—à—å, —á—Ç–æ –Ω–∞ –í–î–ù–• –µ—Å—Ç—å —Ü–µ–ª—ã–π –ø–∞–≤–∏–ª—å–æ–Ω –≤ —á–µ—Å—Ç—å —ç—Ç–æ–π —Ä–µ—Å–ø—É–±–ª–∏–∫–∏? "
                                               "\n–ß—Ç–æ–±—ã –ø–æ–ª—É—á–∏—Ç—å –±–∞–ª–ª—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Å—è –∫ –ø–∞–≤–∏–ª—å–æ–Ω—É ‚Ññ18 –∏ –ø—Ä–∏—Å—ã–ª–∞–π —Ñ–æ—Ç–æ "
                                               "–∫–æ–º–∞–Ω–¥—ã –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –Ω–∞—à–µ–π –≥—Ä—É–ø–ø—ã –í–ö.", reply_markup=men)
    elif "–ë–µ–ª–æ—Ä—É—Å—Å–∏—è" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
        f = open("1q.JPG", 'rb')
        bot.send_photo(message.chat.id, f)
        bot.send_message(message.chat.id, text="–≠—Ç–æ—Ç –∑–Ω–∞–º–µ–Ω–∏—Ç—ã–π –±–µ–ª–æ—Ä—É—Å—Å–∫–∏–π —Ç—Ä–∏–∫–æ—Ç–∞–∂, —Ç—ã –µ–≥–æ —É–∑–Ω–∞–µ—à—å –∏–∑ —Ç—ã—Å—è—á–∏ Calvin Klein."
                                               " –ê —Ç—ã –∑–Ω–∞–µ—à—å, —á—Ç–æ –Ω–∞ –í–î–ù–• –µ—Å—Ç—å —Ü–µ–ª—ã–π –ø–∞–≤–∏–ª—å–æ–Ω –≤ —á–µ—Å—Ç—å —ç—Ç–æ–π —Ä–µ—Å–ø—É–±–ª–∏–∫–∏? "
                                               "\n–ß—Ç–æ–±—ã –ø–æ–ª—É—á–∏—Ç—å –±–∞–ª–ª—ã, –æ—Ç–ø—Ä–∞–≤–ª—è–π—Å—è –∫ –ø–∞–≤–∏–ª—å–æ–Ω—É ‚Ññ18 –∏ –ø—Ä–∏—Å—ã–ª–∞–π —Ñ–æ—Ç–æ "
                                               "–∫–æ–º–∞–Ω–¥—ã –≤ —Å–æ–æ–±—â–µ–Ω–∏—è –Ω–∞—à–µ–π –≥—Ä—É–ø–ø—ã –í–ö.", reply_markup=men)
    elif "–±–µ–ª–æ—Ä—É—Å—Å–∏—è" in dod:
        TO_CHAT_ID = -1001610640629
        bot.send_message(TO_CHAT_ID, text="[–°–ï–†–í–ò–°–ù–û–ï –ú–ï–ù–Æ]\nüü¢üü¢üü¢ " + message.from_user.first_name + ' –æ—Ç–≤–µ—Ç–∏–ª –ø—Ä–∞–≤–∏–ª—å–Ω–æ! –°–º–æ—Ç—Ä–∏ –Ω–∏–∂–µ üîΩ')
        bot.forward_message(TO_CHAT_ID, message.chat.id, message.message_id)
